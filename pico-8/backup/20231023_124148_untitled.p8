pico-8 cartridge // http://www.pico-8.com
version 38
__lua__

function _init()
p={x=60,y=90,speed=4}
buletts={}
end
function _update()
	if(btn(➡️))p.x+=1
	if(btn(⬅️))p.x-=1
	if(btn(⬆️))p.y-=1
	if(btn(⬇️))p.y+=1
	if(btnp(❎))shoot()
 update_bullets()
end
function _draw()
	cls(5)
	map(0,0,0,0)
	--- joueur
	spr(1,p.x,p.y)
	--bullets
	 for i in all (bullets) do
 spr (03,i.x,i.y)
 end
end
-->8
--bullets
function shoot()

	new_bullets={
	x=p.x,
	y=p.y,
	spped=4
	}
	add (bullets, new_bullets)
end

function update_bullets ()
    for i in all (bullets) do
    i.y-=i.speed
    end
    end
